compile_head_wsmvdattspec(_Object,_AttObj,_RefType,[],_WS,[],[]) :- !.

compile_head_wsmvdattspec(Object,AttObj,RefType,[T|L],WS,CodeList,Status) :-
	!,
	compile_head_wsflpathexp(T,WS,TObj,TCodeList,S),
	( S == [] ->
