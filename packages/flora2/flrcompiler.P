/****************************************************************************
  compile_conjunct2list(+ConjunctCode,-CodeList)
****************************************************************************/
compile_conjunct2list(ConjunctCode,CodeList) :-
	conjunct_struct(C1,C2,ConjunctCode),
	!,
	compile_conjunct2list(C1,C1List),
	compile_conjunct2list(C2,C2List),
	append(C1List,C2List,CodeList).

compile_conjunct2list(Code,[Code]) :- !.


